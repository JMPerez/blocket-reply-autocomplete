{
  "name": "Blocket reply autocomplete",
  "manifest_version": 2,
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["http://www.blocket.se/*"],
      "js": ["background.js", "main.js"]
    }
  ],
  "options_page": "options/options.html",
  "background": {
    "scripts": ["background.js"]
  }
}
